{
  "name": "@easy-genomics",
  "description": "Easy Genomics web application to help simplify genomic analysis of sequenced genetic data for bioinformaticians utilizing AWS HealthOmics & NextFlow Tower",
  "scripts": {
    "clobber": "pnpm dlx projen clobber",
    "compile": "pnpm dlx projen compile",
    "default": "pnpm dlx projen default",
    "eject": "pnpm dlx projen eject",
    "eslint": "pnpm dlx projen eslint",
    "package": "pnpm dlx projen package",
    "post-compile": "pnpm dlx projen post-compile",
    "post-upgrade": "pnpm dlx projen post-upgrade",
    "pre-compile": "pnpm dlx projen pre-compile",
    "test": "pnpm dlx projen test",
    "test:watch": "pnpm dlx projen test:watch",
    "upgrade": "pnpm dlx projen upgrade",
    "watch": "pnpm dlx projen watch",
    "projen": "pnpm dlx projen",
    "cicd-test-back-end": "pnpm nx run-many --targets=cicd-test-back-end --projects=@easy-genomics/shared-lib,@easy-genomics/back-end --verbose",
    "cicd-build-deploy-back-end": "pnpm nx run-many --targets=cicd-build-deploy-back-end --projects=@easy-genomics/shared-lib,@easy-genomics/back-end --verbose",
    "cicd-test-front-end": "pnpm nx run-many --targets=cicd-test-front-end --projects=@easy-genomics/shared-lib,@easy-genomics/back-end --verbose",
    "cicd-build-deploy-front-end": "pnpm nx run-many --targets=cicd-build-deploy-front-end --projects=@easy-genomics/shared-lib,@easy-genomics/front-end --verbose",
    "prepare": "husky || true"
  },
  "author": {
    "name": "DEPT Agency",
    "organization": true
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.535.0",
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.3",
    "@commitlint/cz-commitlint": "^18.6.1",
    "@nrwl/devkit": "^15",
    "@nrwl/workspace": "^15",
    "@types/aws-lambda": "^8.10.136",
    "@types/jest": "^29.5.12",
    "@types/node": "^18",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6",
    "@typescript-eslint/parser": "^6",
    "aws-cdk-lib": "^2.134.0",
    "aws-sdk-client-mock": "^3.1.0",
    "aws-sdk-client-mock-jest": "^3.1.0",
    "constructs": "^10.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "nx": "^15",
    "prettier": "^3.2.5",
    "projen": "^0.80.14",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3",
    "validate-branch-name": "^1.3.0"
  },
  "pnpm": {},
  "engines": {
    "node": ">= 18.16.0"
  },
  "main": "lib/index.js",
  "license": "Apache-2.0",
  "homepage": "https://github.com/twobulls/easy-genomics",
  "version": "0.0.0",
  "jest": {
    "moduleNameMapper": {
      "^@BE/(.*)$": "<rootDir>/packages/back-end/src/$1",
      "^@FE/(.*)$": "<rootDir>/packages/front-end/src/$1",
      "^@SharedLib/(.*)$": "<rootDir>/packages/shared-lib/src/$1"
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.ts?(x)",
      "<rootDir>/(test|src)/**/*(*.)@(spec|test).ts?(x)"
    ],
    "clearMocks": true,
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "lcov",
      "clover",
      "cobertura",
      "text"
    ],
    "coverageDirectory": "coverage",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "watchPathIgnorePatterns": [
      "/node_modules/"
    ],
    "reporters": [
      "default"
    ],
    "transform": {
      "^.+\\.[t]sx?$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.dev.json"
        }
      ]
    }
  },
  "types": "lib/index.d.ts",
  "packageManager": "pnpm@8.6.0",
  "//": "~~ Generated by projen. To modify, edit .projenrc.ts and run \"pnpm dlx projen\"."
